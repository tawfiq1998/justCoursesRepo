<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<managed-bean>
		<managed-bean-name>homeBacking</managed-bean-name>
		<managed-bean-class>com.taw.backing.HomeBacking</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>calculaterBacking</managed-bean-name>
		<managed-bean-class>com.taw.backing.CalculateBacking</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>signerBacking</managed-bean-name>
		<managed-bean-class>com.taw.backing.SignerBacking</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>pages/index.xhtml</display-name>
		<from-view-id>/pages/index.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/pages/calculater.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
